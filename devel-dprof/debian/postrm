#!/bin/sh

set -e

if [ "$1" = remove ]
then
    dpkg-divert --remove --package libdevel-dprof-perl --rename \
        --divert /usr/bin/dprofpp.bundled /usr/bin/dprofpp

    dpkg-divert --remove --package libdevel-dprof-perl --rename \
        --divert /usr/share/man/man1/dprofpp.bundled.1.gz /usr/share/man/man1/dprofpp.1.gz
fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
